{
  "name": "proto-verse",
  "version": "1.0.0",
  "description": "ProtoVerse is an open-source project that empowers frontend developers to create a custom API from a JSON file, enabling them to work on their projects without the need for a fully functional backend.",
  "repository": "git@github.com:devoriginal-br/proto-verse.git",
  "author": "DevOriginal <contato@devoriginal.com>",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "tsc --noEmit"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "eslint": "^9.6.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.4.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "*"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1"
  }
}
